{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["src/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "target": "es2021",
    "paths": {
      "src/*": ["src/*"]
    },
    "isolatedModules": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "strictNullChecks": false
  }
}
